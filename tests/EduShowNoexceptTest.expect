#include <exception> // for noexcept transformation

void Fun() noexcept(true)
{
  try 
  {
    int i = 3;
  } catch(...) {
    std::terminate();
  }
}

void Fun2() noexcept(false)
{
  int i = 3;
}

void Fun3() noexcept
{
  try 
  {
    int i = 3;
  } catch(...) {
    std::terminate();
  }
}

void Fun4()
{
  int i = 3;
}
