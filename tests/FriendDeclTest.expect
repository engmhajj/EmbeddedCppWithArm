#include <stdio.h>

class Foo
{
  
  public: 
  inline Foo()
  : mI{0}
  {
  }
  
  inline void Print()
  {
    printf("%d\n", this->mI);
  }
  
  
  private: 
  class Secrect
  {
  };
  
  public: 
  friend class DearFriend;
  private: 
  int mI;
  public: 
  friend void ChangePrivate(Foo &);
};


void ChangePrivate(Foo & i)
{
  ++i.mI;
}

class DearFriend : public Foo::Secrect
{
};


int main()
{
  Foo sPoint = Foo();
  sPoint.Print();
  ChangePrivate(sPoint);
  sPoint.Print();
  return 0;
}
