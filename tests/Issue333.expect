#include <cstdio>
#include <cstdlib>

__attribute__((visibility("default"))) __attribute__((returns_nonnull)) __attribute__((alloc_size(1))) void * operator new(std::size_t sz)
{
  printf("global op new called, size = %zu\n", sz);
  return malloc(sz);
}

__attribute__((visibility("default"))) void operator delete(void * ptr) noexcept
{
  puts("global op delete called");
  free(ptr);
}

int main()
{
  int * p1 = new int;
  delete p1;
  int * p2 = new int[10];
  delete[] p2;
  return 0;
}
