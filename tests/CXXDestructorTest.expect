#define INSIGHTS_USE_TEMPLATE

template<typename T, bool array>
class Alloc
{
  T * data;
  
  public: 
  inline Alloc()
  {
    if(array) {
      this->data = new T[10];
      this->data = new T[10]{1, 2, 3};
    } else {
      this->data = new T;
      this->data = new T{2};
      this->data = new T{2};
    } 
    
  }
  
  inline ~Alloc()
  {
    if(array) {
      delete[] this->data;
    } else {
      delete this->data;
    } 
    
  }
  
};

